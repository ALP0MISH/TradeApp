<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/theme_background"
    tools:context=".presentation.search_screen.FragmentSearch">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/include_search_main_top"
            layout="@layout/include_search_main_top_content"/>


        <include
            android:id="@+id/include_search_main"
            layout="@layout/include_search_main_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>



    </LinearLayout>

</androidx.core.widget.NestedScrollView>


